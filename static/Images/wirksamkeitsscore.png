<svg xmlns="http://www.w3.org/2000/svg" width="500" height="400" viewBox="0 0 500 400">
  <!-- Hintergrund -->
  <rect width="500" height="400" fill="#f0f5ff" rx="10" ry="10"/>
  
  <!-- Gehirn-Symbol -->
  <g transform="translate(50, 100)">
    <circle cx="60" cy="60" r="50" fill="#4285f4" opacity="0.8"/>
    <path d="M40,40 C50,30 70,30 80,40 C90,50 90,70 80,80 C70,90 50,90 40,80 C30,70 30,50 40,40 Z M60,40 C65,45 65,55 60,60 M60,60 C55,65 45,65 40,60 M80,60 C75,55 75,45 80,40 M40,40 C45,35 55,35 60,40" stroke="#ffffff" stroke-width="3" fill="none"/>
    <path d="M50,50 L70,50 M50,70 L70,70" stroke="#ffffff" stroke-width="2" fill="none"/>
  </g>
  
  <!-- Dokument-Symbol -->
  <g transform="translate(200, 100)">
    <rect x="0" y="0" width="80" height="100" fill="#4285f4" rx="5" ry="5"/>
    <line x1="15" y1="20" x2="65" y2="20" stroke="#ffffff" stroke-width="3"/>
    <line x1="15" y1="40" x2="65" y2="40" stroke="#ffffff" stroke-width="3"/>
    <line x1="15" y1="60" x2="65" y2="60" stroke="#ffffff" stroke-width="3"/>
    <line x1="15" y1="80" x2="45" y2="80" stroke="#ffffff" stroke-width="3"/>
  </g>
  
  <!-- Wirksamkeitsscore-Anzeige -->
  <g transform="translate(100, 220)">
    <circle cx="150" cy="80" r="70" fill="white" stroke="#333" stroke-width="2"/>
    <path d="M150,80 L150,30" stroke="#333" stroke-width="3" transform="rotate(-30 150 80)"/>
    
    <!-- Skala -->
    <path d="M150,80 L150,20" stroke="#ddd" stroke-width="1" transform="rotate(-90 150 80)"/>
    <path d="M150,80 L150,20" stroke="#ddd" stroke-width="1" transform="rotate(-45 150 80)"/>
    <path d="M150,80 L150,20" stroke="#ddd" stroke-width="1" transform="rotate(0 150 80)"/>
    <path d="M150,80 L150,20" stroke="#ddd" stroke-width="1" transform="rotate(45 150 80)"/>
    
    <!-- Farbige Bereiche -->
    <path d="M150,80 L150,30 A50,50 0 0,1 190,50 Z" fill="#ff7043" opacity="0.7"/>
    <path d="M150,80 L190,50 A50,50 0 0,1 190,110 Z" fill="#ffca28" opacity="0.7"/>
    <path d="M150,80 L190,110 A50,50 0 0,1 150,130 Z" fill="#66bb6a" opacity="0.7"/>
    
    <!-- Beschriftungen -->
    <text x="100" y="50" font-family="Arial" font-size="12" fill="#333">Niedrig</text>
    <text x="180" y="50" font-family="Arial" font-size="12" fill="#333">Hoch</text>
    <text x="150" y="20" font-family="Arial" font-size="14" font-weight="bold" text-anchor="middle" fill="#333">Wirksamkeitsscore</text>
  </g>
</svg>